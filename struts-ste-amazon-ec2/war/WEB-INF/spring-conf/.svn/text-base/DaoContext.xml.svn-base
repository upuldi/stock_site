        <property name="dataSource" ref="jdbcDataSource"/>
    </bean>

    <bean id="eODForignHoldingDao" class="com.dao.EODForignHoldingDaoJDBC">
        <property name="dataSource" ref="jdbcDataSource"/>
    </bean>

    <bean id="tradeDaoJDBC" class="com.dao.TradeDaoJDBC">
        <property name="dataSource" ref="jdbcDataSource"/>
    </bean>

    <bean id="marketSummeryDao" class="com.dao.MarketSummeryDaoJDBC">
        <property name="dataSource" ref="jdbcDataSource"/>
    </bean>

    <bean id="commentsDao" class="com.dao.CommentsDaoHibernate">
         <property name="sessionFactory" ref="sessionFactory"/>
    </bean>


</beans>